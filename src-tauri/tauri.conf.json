{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Tauri SSH Client",
  "version": "0.1.0",
  "identifier": "com.example.taurisshclient",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Tauri SSH Client",
        "width": 1280,
        "height": 820,
        "minWidth": 980,
        "minHeight": 680,
        "resizable": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": ["../icon.png"]
  }
}
